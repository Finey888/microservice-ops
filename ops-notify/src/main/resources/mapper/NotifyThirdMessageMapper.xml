   <select id="getNotifyMessage" resultMap="BaseResultMap">
    select * from notify_third_message m
    where m.app_id = #{appId} and m.bizId=#{bizId}
  </select>
  
   <select id="getNotifyMessageByKey" resultMap="BaseResultMap">
